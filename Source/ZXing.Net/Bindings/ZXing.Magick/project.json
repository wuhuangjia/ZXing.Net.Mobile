{
	"name": "ZXing.Magick",
	"version": "0.16.4",
	"dependencies": {
		"Magick.NET-Q8-AnyCPU": "7.0.7.300",
		"ZXing.Net": "0.16.2"
   },
	"frameworks": {
		"net20": {
			"buildOptions": {
				"define": [ "NET20" ],
				"debugType": "full"
			}
		},
		"net40": {
			"buildOptions": {
				"define": [ "NET40" ],
				"debugType": "full"
			}
		},
		"netstandard1.3": {
			"dependencies": {
				"NETStandard.Library": "1.6.1",
			},
			"buildOptions": {
				"define": [ "NETSTANDARD" ],
				"debugType": "portable"
			}
		}
	},
	"buildOptions": {
		"optimize": true,
		"xmlDoc": true,
		"platform": "anycpu",
		"publicSign": false,
		"outputName": "zxing.magick",
		"allowUnsafe": true,
		"compile": {
			"includeFiles": [
				"Properties/AssemblyInfo.cs",
				"BarcodeReader.cs",
				"BarcodeReader.Extensions.cs",
				"MagickImageLuminanceSource.cs"
			]
		}
	},
	"configurations": {
		"Debug": {
			"buildOptions": {
				"xmlDoc": false,
				"optimize": false
			}
		}
	}
}
